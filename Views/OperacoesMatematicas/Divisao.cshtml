@{
    ViewData["Title"] = "Divisão";
}

<h1 class="display-4">Operação Matemática Divisão</h1>
<p>Utilize o formulário abaixo para dividir dois números</p>

<form action="ResultadoDivisao" method="post">
    <p>
        <label for="num1">Número 1</label>
        <input type="text" name="num1" id="num1" >
    </p>
    <p>
        <label for="num2">Número 2</label>
        <input type="text" name="num2" id="num2" >
    </p>
    <p>
        <input type="submit" value="Dividir">
    </p>
</form>

